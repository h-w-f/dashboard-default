<?xml version="1.0" encoding="UTF-8"?>
<view>
	<name>Default</name>
	<displayname>Dashboard Application View (Dashboard)</displayname>
	<description>Dashboard Default View</description>
	<version>0.0.2</version>
	<repo>https://github.com/h-w-f/view-dashboard-default</repo>
	<dependencies>
		<application>
			<name>Dashboard</name>
			<version>0.0.1</version>
			<repo>https://github.com/h-w-f/application-dashboard</repo>
		</application>
	</dependencies>
	<settings>
		<cache>false</cache>
		<layout>/layout/view.html</layout>
		<head>
			<title>Dashboard - </title>
			<meta>
				<name>viewport</name>
				<content>width=device-width, initial-scale=1, shrink-to-fit=no</content>
			</meta>
			<meta>
				<name>description</name>
				<content>Dashboard Views</content>
			</meta>
			<meta>
				<name>author</name>
				<content>Guru</content>
			</meta>
			<meta>
				<name>keywords</name>
				<content>HTML, CSS, JavaScript</content>
			</meta>
			<link>
				<href>/views/Dashboard/Default/css/bootstrap.min.css</href>
			</link>
			<script>
				<src>/views/Dashboard/Default/js/jquery-3.5.1.min.js</src>
			</script>
			<script>
				<src>/views/Dashboard/Default/js/bootstrap.min.js</src>
			</script>
		</head>
		<body>
			<params>id="body" class="body"</params>
			<jsscript>
				<![CDATA[
					<script type="text/javascript">
						var x = 10;
					</script>
				]]>
			</jsscript>
		</body>
		<footer>
			<params>class="footer"</params>
			<script>
				<src></src>
			</script>
			<jsscript>
				<![CDATA[
					<script type="text/javascript">
						console.log(x);
					</script>
				]]>
			</jsscript>
		</footer>
	</settings>
</view>
